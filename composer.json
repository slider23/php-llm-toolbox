{
    "name": "slider23/php-llm-toolbox",
    "version": "0.2.0",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "spiral/json-schema-generator": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.0",
        "buggregator/trap": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Slider23\\PhpLlmToolbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slider23\\PhpLlmToolbox\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael Parfenov",
            "email": "slider23@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
